{
	"type": "object",
	"properties": {
		"id": {
			"type": "integer",
			"readOnly": true,
			"example": 3407
		},
		"sourceId": {
			"type": "integer"
		},
		"source": {
			"$ref": "./Source.json",
			"readOnly": true
		},
		"applicationFormId": {
			"$ref": "./id.json"
		},
		"applicationForm": {
			"$ref": "./ApplicationForm.json",
			"readOnly": true
		},
		"proposalsDataFileId": {
			"type": "integer",
			"example": 123
		},
		"proposalsDataFile": {
			"$ref": "./File.json",
			"readOnly": true
		},
		"attachmentsArchiveFileId": {
			"oneOf": [
				{
					"$ref": "./id.json"
				},
				{
					"type": "null"
				}
			]
		},
		"attachmentsArchiveFile": {
			"oneOf": [
				{
					"$ref": "./File.json"
				},
				{
					"type": "null"
				}
			],
			"readOnly": true
		},
		"status": {
			"type": "string",
			"readOnly": true,
			"enum": ["pending", "in_progress", "completed", "failed", "canceled"]
		},
		"createdBy": {
			"description": "The keycloak user id of the PDC user that created this bulk upload",
			"type": "string",
			"format": "uuid",
			"readOnly": true
		},
		"createdByUser": {
			"$ref": "./User.json",
			"readOnly": true
		},
		"createdAt": {
			"type": "string",
			"format": "date-time",
			"readOnly": true
		},
		"logs": {
			"type": "array",
			"properties": {
				"entries": {
					"$ref": "./BulkUploadLog.json"
				}
			},
			"readOnly": true
		}
	},
	"required": [
		"id",
		"sourceId",
		"applicationFormId",
		"proposalsDataFileId",
		"status",
		"createdBy",
		"createdByUser",
		"createdAt",
		"logs"
	]
}

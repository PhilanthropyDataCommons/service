{
	"type": "object",
	"properties": {
		"id": {
			"type": "integer",
			"readOnly": true,
			"example": 3407
		},
		"sourceId": {
			"type": "integer"
		},
		"source": {
			"$ref": "./Source.json",
			"readOnly": true
		},
		"funderShortCode": {
			"type": "string",
			"example": "pdc"
		},
		"funder": {
			"$ref": "./Funder.json",
			"readOnly": true
		},
		"proposalsDataFileId": {
			"type": "integer",
			"example": 123
		},
		"proposalsDataFile": {
			"$ref": "./File.json",
			"readOnly": true
		},
		"attachmentsArchiveFileId": {
			"oneOf": [
				{
					"type": "integer",
					"example": 456
				},
				{
					"type": "null"
				}
			]
		},
		"attachmentsArchiveFile": {
			"oneOf": [
				{
					"$ref": "./File.json"
				},
				{
					"type": "null"
				}
			],
			"readOnly": true
		},
		"status": {
			"type": "string",
			"readOnly": true,
			"enum": ["pending", "in_progress", "completed", "failed", "canceled"]
		},
		"createdBy": {
			"description": "The keycloak user id of the PDC user that created this bulk upload",
			"type": "string",
			"format": "uuid",
			"readOnly": true
		},
		"createdAt": {
			"type": "string",
			"format": "date-time",
			"readOnly": true
		}
	},
	"required": [
		"id",
		"sourceId",
		"proposalsDataFileId",
		"status",
		"createdBy",
		"createdAt"
	]
}

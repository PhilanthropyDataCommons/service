{
	"type": "object",
	"properties": {
		"id": {
			"type": "integer",
			"readOnly": true,
			"example": 3943
		},
		"proposalVersionId": {
			"type": "integer",
			"readOnly": true,
			"example": 3709
		},
		"applicationFormFieldId": {
			"type": "integer",
			"example": 3613
		},
		"applicationFormField": {
			"$ref": "./ApplicationFormField.json",
			"readOnly": true
		},
		"position": {
			"type": "integer",
			"example": 23
		},
		"value": {
			"type": "string",
			"example": "Some arbitrary value"
		},
		"file": {
			"oneOf": [{ "$ref": "./File.json" }, { "type": "null" }],
			"readOnly": true,
			"description": "For file fields, this contains the File object if the file was created by the same user as the proposal version and the value is a valid file ID. Otherwise this property is null or omitted."
		},
		"goodAsOf": {
			"type": ["string", "null"],
			"format": "date-time"
		},
		"isValid": {
			"type": "boolean",
			"example": "true",
			"readOnly": true
		},
		"createdAt": {
			"type": "string",
			"format": "date-time",
			"readOnly": true
		}
	},
	"required": [
		"id",
		"proposalVersionId",
		"applicationFormFieldId",
		"applicationFormField",
		"position",
		"value",
		"goodAsOf",
		"isValid",
		"createdAt"
	]
}

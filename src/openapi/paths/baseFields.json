{
	"get": {
		"operationId": "getBaseFields",
		"summary": "Gets a list of base fields.",
		"tags": ["Base Fields"],
		"security": [
			{
				"auth": []
			}
		],
		"parameters": [
			{
				"$ref": "../components/parameters/baseFieldSensitivityClassificationParam.json"
			}
		],
		"responses": {
			"200": {
				"description": "Base fields currently registered in the PDC. By default, omits 'forbidden' fields.",
				"content": {
					"application/json": {
						"schema": {
							"type": "array",
							"items": {
								"$ref": "../components/schemas/BaseField.json"
							}
						}
					}
				}
			},
			"404": {
				"$ref": "../components/responses/NotFound.json"
			}
		}
	}
}
